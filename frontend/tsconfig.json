{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "types": [
      "@nuxt/types",
      "@types/node",
      "vitest/globals",
    ],
    "typeRoots": ["./node_modules/@types", "./node_modules/typescript", "./node_modules/@typescript-eslint"],
    "jsx": "preserve",
    "skipLibCheck": true,
  }
}
